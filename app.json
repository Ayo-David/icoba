{
  "expo": {
    "name": "icobanew",
    "slug": "icobanew",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon2.png",
    "splash": {
      "image": "./src/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#151B8D"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon2.png",
        "backgroundColor": "#151B8D"
      },
      "package": "com.ayodavid.icobanew"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
